{
  "name": "electron-vite",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "out/main/main.js",
  "homepage": "./",
  "scripts": {
    "electron:build": "npm run vite:electron:build && npm run vite:app:build && electron-builder",
    "vite:electron:build": "tsc && vite build --config electron.vite.config.ts ",
    "vite:app:build": "tsc && vite build --config app.vite.config.ts",
    "electron:preview": "node scripts/build && electron .",
    "electron:js": "tsc --project tsconfig.electron.json",
    "dev": "cross-env NODE_ENV=development node scripts/dev.js",
    "watch": "cross-env NODE_ENV=development watch 'npm run vite:backend:build' ./src"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ip": "^1.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/electron": "^1.6.10",
    "@types/express": "^4.17.17",
    "@types/ip": "^1.1.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "cross-env": "^7.0.3",
    "electron": "^25.3.0",
    "electron-builder": "^24.4.0",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.0"
  }
}
