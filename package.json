  "homepage": "./",
  "scripts": {
    "build": "node scripts/build.js && electron-builder",
    "build:renderer": "vite build --config electron.vite.config.ts ",
    "build:vite:backend": "vite build --config backend.vite.config.ts",
    "transpile:electron": "tsc --project tsconfig.electron.json",
    "preview:electron": "node scripts/build && electron .",
    "dev": "cross-env NODE_ENV=development concurrently \"node scripts/dev.js\"  \"npm run watch\"",
    "dev:vite": "vite --config backend.vite.config.ts",
    "watch": "watch 'vite build --config backend.vite.config.ts' ./src/renderer"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ip": "^1.1.8"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
